                <Parameter Id="%AID%_UP-%T%000007" Name="Watchdog"           ParameterType="%AID%_PT-OnOffYesNo"       Offset="4" BitOffset="1" Text="Watchdog aktivieren" Value="1" />
                <Parameter Id="%AID%_UP-%T%000008" Name="ReadTimeDate"       ParameterType="%AID%_PT-OnOffYesNo"       Offset="4" BitOffset="2" Text="Bei Neustart vom Bus lesen" Value="1" />
                <Parameter Id="%AID%_UP-%T%000009" Name="CombinedTimeDate"   ParameterType="%AID%_PT-CombinedTimeDate" Offset="4" BitOffset="3" Text="Empfangen über" Value="0" />
