          <Dynamic>
            <ChannelIndependentBlock>
              <ParameterBlock Id="%AID%_PB-nnn" Name="General" Text="Allgemein" Icon="cog" HelpContext="Empty">
                <!-- Hidden -->
                <ParameterRefRef RefId="%AID%_P-%T%000010_R-%T%00001001" />

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Allgemein" UIHint="Headline" />
                <!-- Startup Delay pattern -->
                <ParameterBlock Id="%AID%_PB-nnn" Name="StartupDelayCycle" Inline="true" Layout="Grid" HelpContext="Base-Startup">
                  <Rows>
                    <Row Id="%AID%_PB-nnn_R-1" Name="Row1" />
                  </Rows>
                  <Columns>
                    <Column Id="%AID%_PB-nnn_C-1" Name="Col1" Width="45%" />
                    <Column Id="%AID%_PB-nnn_C-2" Name="Col2" Width="15%" />
                    <Column Id="%AID%_PB-nnn_C-3" Name="Col3" Width="20%" />
                  </Columns>
                  <ParameterSeparator Id="%AID%_PS-nnn" Text="  Startverzögerung" Cell="1,1" />
                  <ParameterRefRef RefId="%AID%_UP-%T%000003_R-%T%00000301" Cell="1,2" IndentLevel="1" />
                  <ParameterRefRef RefId="%AID%_UP-%T%000002_R-%T%00000201" Cell="1,3" IndentLevel="1" />
                </ParameterBlock>
                <!-- Heartbeat Delay pattern -->
                <ParameterBlock Id="%AID%_PB-nnn" Name="HeartbeatSendCycle" Inline="true" Layout="Grid" HelpContext="Base-Heartbeat">
                  <Rows>
                    <Row Id="%AID%_PB-nnn_R-1" Name="Row1" />
                  </Rows>
                  <Columns>
                    <Column Id="%AID%_PB-nnn_C-1" Name="Col1" Width="45%" />
                    <Column Id="%AID%_PB-nnn_C-2" Name="Col2" Width="15%" />
                    <Column Id="%AID%_PB-nnn_C-3" Name="Col3" Width="20%" />
                  </Columns>
                  <ParameterSeparator Id="%AID%_PS-nnn" Text="  In Betrieb senden alle" Cell="1,1" />
                  <ParameterRefRef RefId="%AID%_UP-%T%000005_R-%T%00000501" Cell="1,2" IndentLevel="1" />
                  <ParameterRefRef RefId="%AID%_UP-%T%000004_R-%T%00000401" Cell="1,3" IndentLevel="1" />
                </ParameterBlock>
                <choose ParamRefId="%AID%_UP-%T%000005_R-%T%00000501">
                  <when test="!=0">
                    <!-- show heartbeat only if it is parametrized -->
                    <ComObjectRefRef RefId="%AID%_O-%T%000001_R-%T%00000101" />
                  </when>
                </choose>

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Uhrzeit &amp; Datum" UIHint="Headline" />
                <choose ParamRefId="%AID%_P-%T%000010_R-%T%00001001">
                  <when test="0">
                    <!-- Combined Time/Date KO -->
                    <ParameterRefRef RefId="%AID%_UP-%T%000009_R-%T%00000901" HelpContext="Base-CombinedTimeDate" IndentLevel="1" />
                    <choose ParamRefId="%AID%_UP-%T%000009_R-%T%00000901">
                      <when test="0">
                        <!-- Time -->
                        <ComObjectRefRef RefId="%AID%_O-%T%000002_R-%T%00000201" />
                        <!-- Date -->
                        <ComObjectRefRef RefId="%AID%_O-%T%000003_R-%T%00000301" />
                      </when>
                      <when test="1">
                        <!-- Date-Time -->
                        <ComObjectRefRef RefId="%AID%_O-%T%000002_R-%T%00000202" />
                      </when>
                    </choose>
                    <!-- Read Time/Date on startup -->
                    <ParameterRefRef RefId="%AID%_UP-%T%000008_R-%T%00000801" HelpContext="Base-ReadTimeDate" IndentLevel="1" />
                  </when>
                  <when test="1">
                    <ParameterSeparator Id="%AID%_PS-nnn" Text="Die Zeit wird durch einen internen Zeitgeber verwaltet!" UIHint="Information" />
                  </when>
                </choose>

                <ParameterSeparator Id="%AID%_PS-nnn" Text="Erweitert" UIHint="Headline" />
                <ParameterRefRef RefId="%AID%_UP-%T%000006_R-%T%00000601" HelpContext="Base-Diagnose" IndentLevel="1" />
                <choose ParamRefId="%AID%_UP-%T%000006_R-%T%00000601">
                  <when test="1">
                    <!-- show diagnose object just if requested -->
                    <ComObjectRefRef RefId="%AID%_O-%T%000004_R-%T%00000401" />
                  </when>
                </choose>
                <ParameterRefRef RefId="%AID%_UP-%T%000007_R-%T%00000701" HelpContext="Base-Watchdog" IndentLevel="1" />


                <ParameterBlock Id="%AID%_PB-nnn" Name="PoweredBy" Inline="true" Layout="Grid">
                  <Rows>
                    <Row Id="%AID%_PB-nnn_R-1" Name="Row1" />
                  </Rows>
                  <Columns>
                    <Column Id="%AID%_PB-nnn_C-1" Name="Col1" Width="100%" />
                  </Columns>
                  <ParameterRefRef RefId="%AID%_P-%T%000001_R-%T%00000101" Cell="1,1"/>
                </ParameterBlock>
                <ParameterSeparator Id="%AID%_PS-nnn" Text="Dies ist eine OpenKNX Applikation. Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat." />
                <ParameterSeparator Id="%AID%_PS-nnn" Text="Weitere Informationen findest du unter: www.openknx.de - wiki.openknx.de - forum.openknx.de" />


              </ParameterBlock>
            </ChannelIndependentBlock>
